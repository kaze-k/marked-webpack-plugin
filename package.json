{
    "name": "marked-webpack-plugin",
    "version": "1.0.1",
    "description": "A webpack5 plugin for markdown to html",
    "main": "dist/index.js",
    "author": "kaze-k <kerkaze@outlook.com> (https://github.com/kaze-k)",
    "license": "MIT",
    "homepage": "https://github.com/kaze-k/marked-webpack-plugin",
    "scripts": {
        "compile": "node index.mjs",
        "eslint:check": "eslint src",
        "eslint:fix": "eslint --fix src",
        "prettier:check": "prettier --check src",
        "prettier:format": "prettier --write src",
        "test": "webpack --config ./test/webpack.config.babel.js --mode production"
    },
    "files": [
        "dist/"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kaze-k/marked-webpack-plugin.git"
    },
    "bugs": {
        "url": "https://github.com/kaze-k/marked-webpack-plugin/issues"
    },
    "keywords": [
        "wepback",
        "plugin",
        "marked",
        "markdown",
        "html"
    ],
    "peerDependencies": {
        "webpack": ">=5.0.0"
    },
    "dependencies": {
        "ejs": "^3.1.9",
        "html-format": "^1.1.2",
        "marked": "^9.1.2"
    },
    "devDependencies": {
        "@babel/core": "^7.23.2",
        "@babel/preset-env": "^7.23.2",
        "@babel/register": "^7.22.15",
        "@types/ejs": "^3.1.4",
        "@types/node": "^20.8.7",
        "@types/webpack": "^5.28.4",
        "@typescript-eslint/eslint-plugin": "^6.8.0",
        "@typescript-eslint/parser": "^6.8.0",
        "@webpack-cli/generators": "^3.0.7",
        "eslint": "^8.51.0",
        "eslint-config-prettier": "^9.0.0",
        "log-symbols": "^5.1.0",
        "ora": "^7.0.1",
        "prettier": "^3.0.3",
        "typescript": "^5.2.2",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4"
    }
}
